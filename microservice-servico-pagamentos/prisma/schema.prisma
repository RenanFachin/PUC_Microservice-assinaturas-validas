generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Pagamento{
  id String @id @default(uuid())
  codAssinatura String @map("cod_assinatura")
  valorPago Int @map("valor_pago")
  dataPagamento DateTime @map("data_pagamento")


  @@map("pagamentos")
}